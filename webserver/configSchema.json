	            "order": 6,
	            "excludeFromPresets": false,
                "default": 0,
                "userLocked": false,
                "advanced": false
	        },
	        "pressureSensor.maximumAcquisitionTime": {
				"label": "Maximum Acquisition Time",
	            "type": "integer",
	            "description": "The period in seconds over which acquisitions are made for PERIODIC mode, 0=>indefinitely",
	            "order": 7,
	            "excludeFromPresets": false,
                "default": 0,
                "userLocked": false,
                "advanced": false
	        }
	    }
    }
}